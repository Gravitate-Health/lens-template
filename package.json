{
  "name": "lens-template-tests",
  "version": "1.0.0",
  "description": "Test infrastructure for Gravitate Health lens development - All dependencies are for development/testing only",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "tsc"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "@gravitate-health/lens-execution-environment": "^0.0.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "tests/**/*.ts",
      "!tests/**/*.test.ts"
    ]
  },
  "keywords": [
    "fhir",
    "gravitate-health",
    "lens",
    "testing"
  ],
  "author": "Gravitate Health",
  "license": "Apache-2.0"
}
